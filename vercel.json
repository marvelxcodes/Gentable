{
    "version": 2,
    "buildCommand": "pnpm build",
    "installCommand": "pnpm install",
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
      }
    ]
  }
  